{
  "name": "fg-cw-acceptance-tests",
  "version": "0.0.0",
  "type": "module",
  "description": "",
  "engines": {
    "node": ">=22.13.1"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "aliases": {
    "~": ".",
    "page-objects": "test/page-objects",
    "components": "test/components"
  },
  "scripts": {
    "clean": "rm -rf allure-results && rm -rf allure-report",
    "test": "npm run clean && wdio run wdio.conf.js --cucumberOpts.tagExpression='@accessibility'",
    "test:local": "npm run clean && wdio run wdio.local.conf.js",
    "test:tags": "npm run clean && SELECTED_TAGS='@flying-pigs' wdio run wdio.local.conf.js --cucumberOpts.tags='@flying-pigs'",
    "test:local:debug": "DEBUG=true npm run test:local",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "format": "prettier --write '**/*.{js,md,json,config.js}'",
    "format:check": "prettier --check '**/*.{js,md,json,config.js}'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npm run setup:husky",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "report": "allure generate allure-results --clean",
    "accessibility": "npm run clean && SELECTED_TAGS='@accessibility' wdio run wdio.accessibility.conf.js --cucumberOpts.tags='@accessibility'",
    "accessibility:report": "node --no-warnings generate-accessibility-report.js",
    "report:publish": "npm run report; ./bin/publish-tests.sh"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/register": "7.25.9",
    "@cucumber/cucumber": "11.3.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.3.12",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@wdio/allure-reporter": "9.10.1",
    "@wdio/browserstack-service": "9.14.0",
    "@wdio/cli": "9.10.1",
    "@wdio/cucumber-framework": "9.10.1",
    "@wdio/globals": "9.10.1",
    "@wdio/local-runner": "9.10.1",
    "@wdio/mocha-framework": "9.10.1",
    "@wdio/spec-reporter": "9.10.1",
    "ajv": "^8.17.1",
    "allure-commandline": "2.32.2",
    "aws4": "^1.13.2",
    "babel-plugin-module-resolver": "5.0.2",
    "chromedriver": "^138.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-wdio": "8.24.12",
    "esm-module-alias": "2.2.1",
    "global-agent": "3.0.0",
    "hapi-pino": "^12.1.0",
    "hapi-pulse": "^3.0.1",
    "hapi-swagger": "^17.3.2",
    "husky": "9.0.11",
    "joi": "^17.13.3",
    "prettier": "3.2.5",
    "vitest": "^3.1.1",
    "wdio-chromedriver-service": "8.1.1"
  },
  "dependencies": {
    "@hapi/wreck": "18.1.0",
    "axe-core": "4.10.3",
    "date-fns": "4.1.0",
    "vite": "6.3.2"
  }
}
